<div class="header">
  <%= link_to :back do%>
  <i class="fa fa-angle-left" style="color:black; padding:10px"></i>
<% end %>
<h1 class="text-center">Edit profile</h1>
</div>

<div class="container new_form">

<%= simple_form_for(@profile) do |f| %>
  <%= f.input :gender%>
  <%= f.input :date_of_birth%>
  <%= f.input :allergies %>
  <%= f.input :medication %>
  <%= f.input :dosage %>
  <%= f.input :times_per_day %>
  <%= f.input :vaccinations %>
  <%= f.input :blood_type %>
  <%= f.input :race %>
  <%= f.button :submit, "Submit" ,class:"btn btn-dark" %>
<% end %>
</div>
