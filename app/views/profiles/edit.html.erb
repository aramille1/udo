<div class="container new_form">
<h1>Edit Profile</h1>

<%= simple_form_for(@profile) do |f| %>
  <%= f.input :date_of_birth%>
  <%= f.input :allergies %>
  <%= f.input :medication %>
  <%= f.input :dosage %>
  <%= f.input :times_per_day %>
  <%= f.input :vaccinations %>
  <%= f.input :blood_type %>
  <%= f.input :race %>
  <%= f.button :submit, "Edit" ,class:"btn btn-dark" %>
<% end %>
</div>
